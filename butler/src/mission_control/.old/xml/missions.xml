<missions>
	<mission>
		<name="S1ToTable"/>
		<startGoal="Station1"/>
		<targetGoal="Table"/>
		<searchObject>
			<type="cup"/>
			<color="orange"/>
		</searchObject>
		<onSuccess>
			<action="putOnGoal">
			<sound="success">
			<nextMission="S2ToTable">
